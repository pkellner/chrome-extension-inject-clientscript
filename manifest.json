{
  "name": "injected script test",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": ["activeTab", "contextMenus"],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "run_at": "document_idle",
      "js": ["content-script.js"]
    }
  ],
  "browser_action": {
  },
  "web_accessible_resources": ["*"]
}
